# SES Inventory Environment Variables
# Copy this file to .env and fill in your values

# Anthropic API (for AI coin identification)
REACT_APP_ANTHROPIC_API_KEY=your_anthropic_key_here

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=123456789
REACT_APP_FIREBASE_APP_ID=1:123456789:web:abcdef

# eBay API (optional)
REACT_APP_EBAY_APP_ID=your_ebay_app_id
REACT_APP_EBAY_CERT_ID=your_ebay_cert_id

# GoldAPI.io (optional backup for spot prices)
REACT_APP_GOLD_API_KEY=your_goldapi_key
